{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "src/routes/index.js",
    "src/app/index.js",
    "src/pages/SignUp/index.js",
    "src/routes/auth.routes.js"
  ]
}
